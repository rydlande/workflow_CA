{
  "name": "workflow_ca",
  "version": "1.0.0",
  "description": "CA in Workflow",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff & Eirin Rydland",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "jest-localstorage-mock": "^2.4.26",
    "lint-staged": "^15.0.2",
    "live-server": "^1.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rydlande/workflow_CA.git"
  },
  "bugs": {
    "url": "https://github.com/rydlande/workflow_CA/issues"
  },
  "homepage": "https://github.com/rydlande/workflow_CA#readme",
  "lint-staged": {
    "src/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
